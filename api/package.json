{
  "name": "omnipair-data-api",
  "version": "1.0.0",
  "description": "TypeScript Express API with PostgreSQL integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "fill-ema": "npm run build && node dist/scripts/fillEmaPrices.js",
    "verify-ema": "npm run build && node dist/scripts/fillEmaPrices.js verify",
    "ema-both": "npm run build && node dist/scripts/fillEmaPrices.js both"
  },
  "keywords": ["typescript", "express", "postgresql", "api"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@types/pg": "^8.10.2",
    "@types/cors": "^2.8.13",
    "typescript": "^5.1.6",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1"
  }
}
