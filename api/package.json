{
  "name": "omnipair-data-api",
  "version": "1.0.0",
  "description": "TypeScript Express API with PostgreSQL integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc && npm run copy-idl",
    "copy-idl": "mkdir -p dist/idl && cp src/idl/*.json dist/idl/ 2>/dev/null || true",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "fill-ema": "npm run build && node dist/scripts/fillEmaPrices.js",
    "verify-ema": "npm run build && node dist/scripts/fillEmaPrices.js verify",
    "ema-both": "npm run build && node dist/scripts/fillEmaPrices.js both"
  },
  "keywords": [
    "typescript",
    "express",
    "postgresql",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.4.0",
    "@metaplex-foundation/umi": "^1.4.1",
    "@metaplex-foundation/umi-bundle-defaults": "^1.4.1",
    "@metaplex-foundation/umi-web3js-adapters": "^1.4.1",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@types/pg": "^8.10.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
